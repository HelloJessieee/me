{
  "public": "build",
  "rewrites": [
    {
      "source": "/me/**",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/static/css/**",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css"
        }
      ]
    },
    {
      "source": "/static/js/**",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    },
    {
      "source": "/manifest.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/manifest+json"
        }
      ]
    }
  ]
} 